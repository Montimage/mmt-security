<beginning>
<!-- Property 49: Detection of unknown protocols-->
<property value="THEN" delay_units="ms" delay_min="0" delay_max="0" property_id="49" type_property="ATTACK" 
    description="Detection of unknown protocols">
    <event value="COMPUTE" event_id="1" 
           description="Packet representing an unknown protocol"
           boolean_expression="(#is_exist(unknown.p_data))"/>
    <event value="COMPUTE" event_id="2" 
           description="IP source, IP destination"
           boolean_expression="(ip.src != ip.dst)"/> 
</property>
</beginning>


