<beginning>
<!-- Property 57: Simple rule to detect mongo traffic 
-->
	<property value="THEN" delay_units="ms" delay_min="0" delay_max="0" property_id="57" type_property="ATTACK" 
    description="Mongo traffic">
      <event value="COMPUTE" event_id="1" 
             description="Mongo packet"
             boolean_expression="(mongodbwire.packet_count != 0)"/>
      <event value="COMPUTE" event_id="2" 
             description="IP packet"
             boolean_expression="(ip.packet_count != 0)"/>
</property>
</beginning>
