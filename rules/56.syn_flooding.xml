<beginning>
<!-- Property 56: Probable SYN flooding attack.
-->
	<property value="THEN" delay_units="ms" delay_min="0+" delay_max="100" property_id="56" type_property="SECURITY" 
    description="Probable SYN flooding attack (Half TCP handshake without TCP RST)">
    <operator value="THEN" delay_units="ms" delay_min="0+" delay_max="10">
      <event value="COMPUTE" event_id="1" 
             description="TCP SYN packet"
             boolean_expression="((tcp.flags == 2)&amp;&amp;(ip.src != ip.dst))"/>
      <event value="COMPUTE" event_id="2" 
             description="TCP [SYN, ACK] packet"
             boolean_expression="((tcp.flags == 18)&amp;&amp;((ip.src == ip.dst.1)&amp;&amp;(ip.dst == ip.src.1)))"/>
    </operator>
    <event value="COMPUTE" event_id="3"
             description="TCP RST packet to finish TCP handshake"
             boolean_expression="((tcp.rst == 1)&amp;&amp;((ip.src == ip.src.1)&amp;&amp;(ip.dst == ip.dst.1)))"/>
</property>
</beginning>
